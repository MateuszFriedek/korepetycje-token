<div id="<%= dom_id student %>">
  <p>
    <strong>Name:</strong>
    <%= student.name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= student.email %>
  </p>

  <p>
    <strong>Phone number:</strong>
    <%= student.phone_number %>
  </p>

  <p>
    <strong>Start date:</strong>
    <%= student.start_date %>
  </p>

  <p>
    <strong>User:</strong>
    <%= student.user_id %>
  </p>

</div>
